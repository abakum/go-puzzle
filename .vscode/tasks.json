{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "$GOARCH=386;cd ${fileDirname};go build",
			"command": "cd ${fileDirname};go build",
			"options": {
				"env": {
					"GOARCH": "386"
				}
			},
			"problemMatcher": [
				"$go"
			],
			"group": "build"
		},
		{
			"type": "shell",
			"label": "$GOARCH=amd64;cd ${fileDirname};go build",
			"command": "cd ${fileDirname};go build",
			"options": {
				"env": {
					"GOARCH": "amd64"
				}
			},
			"problemMatcher": [
				"$go"
			],
			"group": "build"
		},
	]
}